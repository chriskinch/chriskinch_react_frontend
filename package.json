{
  "name": "chriskinch.com_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.6",
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link-http": "^1.1.0",
    "axios": "^0.16.2",
    "flux": "^3.1.3",
    "google-maps": "^3.2.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.2.0",
    "react-scripts": "1.0.14",
    "sass-mq": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path ./node_modules/sass-mq",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./node_modules/sass-mq",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {}
}
